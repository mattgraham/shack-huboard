    <div class="repo-settings">
      <nav class="repo-settings--nav">
        <li>
          {{#link-to 'settings.index' }}
            General
          {{/link-to}}
        </li>
        <li>
          {{#link-to 'settings.links' }}
            Links
          {{/link-to}}
        </li>
        <li>
          {{#link-to 'settings.integrations' }}
            Integrations
          {{/link-to}}
        </li>
      </nav>
      <div class="repo-settings--content">
        {{outlet}}
      </div>
    </div>
  </div>
</div>


