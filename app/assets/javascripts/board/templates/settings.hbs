<div class="main-toolbar">
  {{#if App.repo.is_collaborator}}
  <div>
  </div>
  <div class="create-button">
    <span class="dropdown">
      <a data-toggle="dropdown" class="dropdown-toggle hb-icon-link" href="#/settings">
        <i class="ui-icon ui-icon-18 ui-icon-gear"></i>
      </a>
    </span>
  </div>
  {{/if}}
</div>
<div id="main-stage" class="main-content">
